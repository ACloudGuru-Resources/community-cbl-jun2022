import java.util.List;
import java.util.ArrayList;

countries = new ArrayList();
countries.add(&quot;USA&quot;);
countries.add(&quot;IRL&quot;);
countries.add(&quot;IND&quot;);
countries.add(&quot;CAN&quot;);
countries.add(&quot;ISL&quot;);
countries.add(&quot;FRA&quot;);

